<%= @participant.name %>
<%= @participant.poker_session_participant_estimate&.value %>

<% @poker_session.poker_session_participants.each do |participant| %>
  <%= participant.name %>
  <%= participant.poker_session_participant_estimate&.value %>
<% end %>

<% @poker_session.estimates.split(",").each do |estimate| %>
  <%= button_to estimate, poker_session_poker_session_participant_poker_session_participant_estimates_path(@poker_session.id, @participant.id), method: :post,  params: { value: estimate } %>
<% end %>
