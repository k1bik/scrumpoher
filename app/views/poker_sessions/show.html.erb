<%= turbo_stream_from dom_id(@poker_session) %>

<div class="d-flex">
  <% @poker_session.estimates.split(",").each do |estimate| %>
    <%= button_to estimate, poker_session_poker_session_participant_poker_session_participant_estimates_path(@poker_session.id, @participant.id), method: :post, class: "btn btn-outline-secondary",  params: { value: estimate } %>
  <% end %>
</div>

<%= turbo_frame_tag :table do %>
  <%= render "table", poker_session: @poker_session %>
<% end %>
